{
  "name": "@neuronutrition/shared-questionnaires",
  "version": "1.0.0",
  "description": "Shared questionnaires library for patient and practitioner apps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rimraf dist"
  },
  "keywords": [
    "questionnaires",
    "neuronutrition",
    "shared"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.3.3"
  }
}
